{
  "private": true,
  "name": "studio-test",
  "description": "Test package for Prisma Studio CLI command",
  "type": "module",
  "scripts": {
    "studio:bun": "bun --bun run ../../packages/cli/build/index.js studio --config ./prisma.config.ts",
    "studio:bun:sqlite": "PROVIDER=sqlite pnpm studio:bun",
    "studio:deno": "deno run -A --unstable-detect-cjs ../../packages/cli/build/index.js studio --config ./prisma.config.ts",
    "studio:deno:sqlite": "PROVIDER=sqlite pnpm studio:deno",
    "studio:node": "dotenvx run -- tsx ../../packages/cli/build/index.js studio --config ./prisma.config.ts",
    "studio:node:debug": "DEBUG=prisma:* pnpm studio:node",
    "studio:node:mysql": "PROVIDER=mysql pnpm studio:node",
    "studio:node:mysql:debug": "PROVIDER=mysql pnpm studio:node:debug",
    "studio:node:postgres": "PROVIDER=postgres pnpm studio:node",
    "studio:node:postgres:debug": "PROVIDER=postgres pnpm studio:node:debug",
    "studio:node:sqlite": "PROVIDER=sqlite pnpm studio:node",
    "studio:node:sqlite:debug": "PROVIDER=sqlite pnpm studio:node:debug"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/config": "../../packages/config",
    "mysql2": "^3.15.3"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.29.3",
    "tsx": "^4.20.6",
    "typescript": "5.9.3"
  }
}
