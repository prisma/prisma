generator client {
  provider        = "prisma-client"
  engineType      = "library"
  previewFeatures = ["driverAdapters"]
  output         = "./generated/client"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id    String @id @default(uuid())
  email String @unique
}