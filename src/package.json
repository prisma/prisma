    "node": ">=10.13"
  },
  "devDependencies": {
    "@sindresorhus/slugify": "1.1.0",
    "@types/debug": "4.1.5",
    "@types/graphviz": "0.0.32",
    "@types/node": "12.19.9",
    "@types/redis": "2.8.28",
    "arg": "5.0.0",
    "batching-toposort": "1.2.0",
    "chalk": "4.1.0",
    "debug": "4.3.1",
    "execa": "5.0.0",
    "globby": "11.0.1",
    "graphviz": "0.0.9",
    "node-fetch": "2.6.1",
