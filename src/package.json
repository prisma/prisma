{
  "name": "prisma2-dev-environment",
  "version": "1.0.1",
  "main": "index.js",
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/node": "13.13.12",
    "@types/redis": "2.8.22",
    "arg": "4.1.3",
    "batching-toposort": "1.2.0",
    "chalk": "4.1.0",
    "debug": "4.1.1",
    "execa": "4.0.2",
    "globby": "11.0.1",
    "node-fetch": "2.6.0",
    "p-map": "4.0.0",
    "p-reduce": "2.1.0",
    "redis": "3.0.2",
    "redis-lock": "0.1.4",
    "semver": "7.3.2",
    "ts-node": "8.10.2",
    "typescript": "3.9.5"
  },
  "scripts": {
    "preinstall": "node -e \"!process.env.npm_config_user_agent.startsWith('pnpm/') && !console.log('Use pnpm install to install dependencies in this repository\\n') && process.exit(1)\"",
    "setup": "ts-node scripts/setup.ts",
    "build": "ts-node scripts/setup.ts --build",
    "download": "pnpm --prefix=./packages/engine-core/ run download && pnpm --prefix=./packages/client/ run download && pnpm --prefix=./packages/sdk/ run download && pnpm --prefix=./packages/migrate/ run download",
    "dry": "ts-node scripts/ci/publish.ts --dry-run",
    "publish-all": "ts-node scripts/ci/publish.ts --publish",
    "test": "ts-node scripts/ci/publish.ts --test",
    "all": "ts-node scripts/ci/all.ts",
    "status": "ts-node scripts/ci/publish.ts --status",
    "pull": "ts-node scripts/ci/publish.ts --pull"
  }
}
