    }
  },
  "lint-staged": {
    "*.{js,ts,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,ts}": [
      "eslint"
    ]
  },
  "dependencies": {
    "pkg-up": "^3.1.0"
  }
