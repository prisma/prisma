datasource db {
  provider = "sqlite"
  url      = "file:dev-tmp.db"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["atomicNumberOperations"]
}

// / User model comment
model User {
  id         String  @default(uuid()) @id
  email      String  @unique
  // / name comment
  name       String?
  countInt   Int?
  countFloat Float?
}
