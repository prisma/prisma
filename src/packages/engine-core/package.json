  },
  "files": [
    "!**/__tests__",
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,ts}": [
      "eslint"
    ]
  }
