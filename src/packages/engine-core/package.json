    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "strip-ansi": "6.0.0",
    "ts-jest": "26.4.0",
    "typescript": "4.0.3"
  },
  "scripts": {
    "build": "tsc -d",
