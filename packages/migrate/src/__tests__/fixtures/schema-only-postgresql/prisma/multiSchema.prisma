generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["multiSchema"]
}

datasource my_db {
    provider = "postgresql"
    url      = env("TEST_POSTGRES_URI_MIGRATE")
    schemas  = ["schema1", "schema2"]
}

model Blog {
    id          Int @id
    viewCount20 Int

    @@schema("schema1")
}
