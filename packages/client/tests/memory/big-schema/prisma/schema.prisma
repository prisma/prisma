datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
}

model alltime_standing_entries {
    id                  Int      @id @default(autoincrement())
    alltime_standing_id Int
    team_id             Int
    pos                 Int?
    played              Int?
    won                 Int?
    lost                Int?
    drawn               Int?
    goals_for           Int?
    goals_against       Int?
    pts                 Int?
    recs                Int?
    comments            String?
    created_at          DateTime
    updated_at          DateTime
}

model alltime_standings {
    id         Int      @id @default(autoincrement())
    key        String
    title      String
    created_at DateTime
    updated_at DateTime
}

model assocs {
    id               Int      @id @default(autoincrement())
    key              String
    title            String
    since            Int?
    web              String?
    country_id       Int?
    national         Boolean  @default(dbgenerated("'f'"))
    continental      Boolean  @default(dbgenerated("'f'"))
    intercontinental Boolean  @default(dbgenerated("'f'"))
    created_at       DateTime
    updated_at       DateTime
}

model assocs_assocs {
    id         Int      @id @default(autoincrement())
    assoc1_id  Int
    assoc2_id  Int
    created_at DateTime
    updated_at DateTime
}

model badges {
    id         Int      @id @default(autoincrement())
    team_id    Int
    league_id  Int
    season_id  Int
    title      String
    created_at DateTime
    updated_at DateTime
}

model cities {
    id         Int      @id @default(autoincrement())
    name       String
    key        String
    place_id   Int
    code       String?
    alt_names  String?
    country_id Int
    region_id  Int?
    city_id    Int?
    pop        Int?
    popm       Int?
    area       Int?
    m          Boolean  @default(dbgenerated("'f'"))
    c          Boolean  @default(dbgenerated("'f'"))
    d          Boolean  @default(dbgenerated("'f'"))
    created_at DateTime
    updated_at DateTime
}

model continents {
    id         Int      @id @default(autoincrement())
    name       String
    slug       String
    key        String
    place_id   Int
    alt_names  String?
    created_at DateTime
    updated_at DateTime
}

model countries {
    id           Int      @id @default(autoincrement())
    name         String
    slug         String
    key          String
    place_id     Int
    code         String
    alt_names    String?
    pop          Int
    area         Int
    continent_id Int?
    country_id   Int?
    s            Boolean  @default(dbgenerated("'f'"))
    c            Boolean  @default(dbgenerated("'f'"))
    d            Boolean  @default(dbgenerated("'f'"))
    motor        String?
    iso2         String?
    iso3         String?
    fifa         String?
    net          String?
    wikipedia    String?
    created_at   DateTime
    updated_at   DateTime
}

model event_standing_entries {
    id                Int      @id @default(autoincrement())
    event_standing_id Int
    team_id           Int
    pos               Int?
    played            Int?
    won               Int?
    lost              Int?
    drawn             Int?
    goals_for         Int?
    goals_against     Int?
    pts               Int?
    comments          String?
    created_at        DateTime
    updated_at        DateTime
}

model event_standings {
    id         Int      @id @default(autoincrement())
    event_id   Int
    created_at DateTime
    updated_at DateTime
}

model events {
    id         Int       @id @default(autoincrement())
    key        String
    league_id  Int
    season_id  Int
    start_at   DateTime
    end_at     DateTime?
    team3      Boolean   @default(dbgenerated("'t'"))
    sources    String?
    config     String?
    created_at DateTime
    updated_at DateTime
}

model events_grounds {
    id         Int      @id @default(autoincrement())
    event_id   Int
    ground_id  Int
    created_at DateTime
    updated_at DateTime
}

model events_teams {
    id         Int      @id @default(autoincrement())
    event_id   Int
    team_id    Int
    created_at DateTime
    updated_at DateTime
}

model games {
    id           Int       @id @default(autoincrement())
    key          String?
    round_id     Int
    pos          Int
    group_id     Int?
    team1_id     Int
    team2_id     Int
    play_at      DateTime
    postponed    Boolean   @default(dbgenerated("'f'"))
    play_at_v2   DateTime?
    play_at_v3   DateTime?
    ground_id    Int?
    city_id      Int?
    knockout     Boolean   @default(dbgenerated("'f'"))
    home         Boolean   @default(dbgenerated("'t'"))
    score1       Int?
    score2       Int?
    score1et     Int?
    score2et     Int?
    score1p      Int?
    score2p      Int?
    score1i      Int?
    score2i      Int?
    score1ii     Int?
    score2ii     Int?
    next_game_id Int?
    prev_game_id Int?
    winner       Int?
    winner90     Int?
    created_at   DateTime
    updated_at   DateTime
}

model goals {
    id         Int      @id @default(autoincrement())
    person_id  Int
    game_id    Int
    team_id    Int
    minute     Int?
    offset     Int      @default(0)
    score1     Int?
    score2     Int?
    penalty    Boolean  @default(dbgenerated("'f'"))
    owngoal    Boolean  @default(dbgenerated("'f'"))
    created_at DateTime
    updated_at DateTime
}

model grounds {
    id         Int      @id @default(autoincrement())
    key        String
    title      String
    synonyms   String?
    country_id Int
    city_id    Int?
    since      Int?
    capacity   Int?
    address    String?
    created_at DateTime
    updated_at DateTime
}

model group_standing_entries {
    id                Int      @id @default(autoincrement())
    group_standing_id Int
    team_id           Int
    pos               Int?
    played            Int?
    won               Int?
    lost              Int?
    drawn             Int?
    goals_for         Int?
    goals_against     Int?
    pts               Int?
    comments          String?
    created_at        DateTime
    updated_at        DateTime
}

model group_standings {
    id         Int      @id @default(autoincrement())
    group_id   Int
    created_at DateTime
    updated_at DateTime
}

model groups {
    id         Int      @id @default(autoincrement())
    event_id   Int
    title      String
    pos        Int
    created_at DateTime
    updated_at DateTime
}

model groups_teams {
    id         Int      @id @default(autoincrement())
    group_id   Int
    team_id    Int
    created_at DateTime
    updated_at DateTime
}

model langs {
    id         Int      @id @default(autoincrement())
    key        String
    name       String
    created_at DateTime
    updated_at DateTime
}

model leagues {
    id         Int      @id @default(autoincrement())
    key        String
    title      String
    country_id Int?
    club       Boolean  @default(dbgenerated("'f'"))
    created_at DateTime
    updated_at DateTime
}

model logs {
    id         Int      @id @default(autoincrement())
    msg        String
    level      String
    app        String?
    tag        String?
    pid        Int?
    tid        Int?
    ts         String?
    created_at DateTime
    updated_at DateTime
}

model names {
    id         Int      @id @default(autoincrement())
    name       String
    place_id   Int
    lang       String   @default("en")
    created_at DateTime
    updated_at DateTime
}

model persons {
    id             Int       @id @default(autoincrement())
    key            String
    name           String
    synonyms       String?
    code           String?
    born_at        DateTime?
    city_id        Int?
    region_id      Int?
    country_id     Int?
    nationality_id Int?
