    updated_at DateTime
}

model seasons {
    id         Int      @id @default(autoincrement())
    key        String
    title      String
    created_at DateTime
    updated_at DateTime
}

model taggings {
    id            Int      @id @default(autoincrement())
    tag_id        Int
    taggable_id   Int
    taggable_type String
    created_at    DateTime
    updated_at    DateTime
}

model tags {
    id         Int      @id @default(autoincrement())
    key        String
    slug       String
    name       String?
    grade      Int      @default(1)
    parent_id  Int?
    created_at DateTime
    updated_at DateTime
}

model teams {
    id         Int      @id @default(autoincrement())
    key        String
    title      String
    title2     String?
    code       String?
    synonyms   String?
    country_id Int
    city_id    Int?
    club       Boolean  @default(false)
    since      Int?
    address    String?
    web        String?
    assoc_id   Int?
    national   Boolean  @default(false)
    created_at DateTime
    updated_at DateTime
}

model usages {
    id         Int      @id @default(autoincrement())
    country_id Int
    lang_id    Int
    official   Boolean  @default(dbgenerated("'t'"))
    minor      Boolean  @default(dbgenerated("'f'"))
    percent    Float?
    created_at DateTime
    updated_at DateTime
}
