    updated_at     DateTime
}

model places {
    id         Int      @id @default(autoincrement())
    name       String
    kind       String
    lat        Float?
    lng        Float?
    created_at DateTime
    updated_at DateTime
}

model props {
    id         Int      @id @default(autoincrement())
    key        String
    value      String
    kind       String?
    created_at DateTime
    updated_at DateTime
}

model regions {
    id         Int      @id @default(autoincrement())
    name       String
    key        String
    place_id   Int
    code       String?
    abbr       String?
    iso        String?
    nuts       String?
    alt_names  String?
    country_id Int
