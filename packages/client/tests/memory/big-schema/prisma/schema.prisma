    created_at DateTime
    updated_at DateTime
}

model rosters {
    id         Int      @id @default(autoincrement())
    person_id  Int
    team_id    Int
    event_id   Int?
    pos        Int
    created_at DateTime
    updated_at DateTime
}

model rounds {
    id         Int       @id @default(autoincrement())
    event_id   Int
    title      String
    title2     String?
    pos        Int
    knockout   Boolean   @default(dbgenerated("'f'"))
    start_at   DateTime
    end_at     DateTime?
    auto       Boolean   @default(dbgenerated("'t'"))
