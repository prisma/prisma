// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters", "multiSchema"]
}

datasource db {
  provider = "postgres"
  url      = env("POSTGRES_URL")
  schemas = ["base", "public"]
}

model User {
  id String @id

  @@schema("base")
}
