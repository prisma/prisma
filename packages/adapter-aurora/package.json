  "dependencies": {
    "@prisma/driver-adapter-utils": "workspace:*"
  },
  "devDependencies": {
    "@aws-sdk/client-rds-data": "3.600.0",
    "@swc/core": "1.6.3",
    "@swc/jest": "0.2.36",
    "jest": "29.7.0",
    "jest-junit": "16.0.0"
  },
