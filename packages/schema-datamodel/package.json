{
  "name": "@prisma/schema-datamodel",
  "version": "0.0.0",
  "description": "TypeScript implementation of Prisma's abstract data model for schema generation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prisma/prisma.git",
    "directory": "packages/schema-datamodel"
  },
  "license": "Apache-2.0",
  "dependencies": {},
  "devDependencies": {
    "vitest": "3.0.9",
    "@types/node": "18.19.76"
  },
  "scripts": {
    "dev": "DEV=true tsx helpers/build.ts",
    "build": "tsx helpers/build.ts",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false
}
