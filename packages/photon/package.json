{
  "name": "@prisma/client",
  "version": "2.0.0-alpha.1013",
  "description": "Prisma Client is an auto-generated and type-safe query builder for Node.js & TypeScript that's tailored to your data.",
  "keywords": [
    "prisma2",
    "prisma",
    "client",
    "query",
    "database",
    "sql",
    "postgresql",
    "mysql",
    "sqlite",
    "mariadb",
    "typescript",
    "orm",
    "query-builder"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "license": "Apache-2.0",
  "scripts": {
    "tsc": "tsc -d",
    "build": "pnpm run cleanup && pnpm run tsc && pnpm run ncc:runtime && pnpm run ncc:generator && cp scripts/backup-index.js index.js && cp scripts/backup-index.d.ts index.d.ts &&  cp scripts/any-index.js runtime/index.d.ts",
    "test": "jest",
    "download": "node scripts/download.js",
    "generate": "node scripts/postinstall.js",
    "postinstall": "node scripts/postinstall.js",
    "precommit": "lint-staged && jest",
    "ncc:runtime": "node scripts/build-runtime.js",
    "cleanup": "rm -f index.d.ts && cp scripts/backup-index.js index.js && cp scripts/backup-index.d.ts index.d.ts",
    "ncc:generator": "rm -rf generator-build && ncc build dist/generator.js -o generator-build --minify --source-map && rm -f generator-build/index.js.bak && chmod +x generator-build/index.js",
    "prepare": "pnpm run cleanup && cp scripts/backup-index.js index.js && cp scripts/backup-index.d.ts index.d.ts && node scripts/download.js || echo \"\"",
    "prepublishOnly": "pnpm run build"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "tslint --project tsconfig.json -c tslint.json --fix",
      "prettier --write",
      "git add"
    ]
  },
  "prisma": {
    "version": "latest"
  },
  "files": [
    "runtime/*.js",
    "runtime/*.d.ts",
    "runtime/**/*.js",
    "runtime/**/*.d.ts",
    "runtime/utils/*",
    "runtime/dist/*",
    "scripts",
    "generator-build",
    "index.js",
    "index.d.ts"
  ],
  "devDependencies": {
    "@apexearth/copy": "^1.4.4",
    "@prisma/engine-core": "workspace:*",
    "@prisma/fetch-engine": "workspace:*",
    "@prisma/generator-helper": "workspace:*",
    "@prisma/get-platform": "workspace:*",
    "@prisma/sdk": "workspace:*",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^24.9.1",
    "@types/js-levenshtein": "^1.1.0",
    "@types/node": "^12.12.31",
    "@types/node-fetch": "^2.5.5",
    "@types/pg": "^7.14.3",
    "@zeit/ncc": "^0.20.5",
    "arg": "^4.1.3",
    "benchmark": "^2.1.4",
    "chalk": "^3.0.0",
    "del": "5.1.0",
    "dotenv": "^8.2.0",
    "execa": "3.3.0",
    "fast-clone": "^1.5.13",
    "fast-copy": "^2.0.5",
    "flat-map-polyfill": "^0.3.8",
    "fs-monkey": "^0.3.3",
    "get-own-enumerable-property-symbols": "^3.0.2",
    "indent-string": "^4.0.0",
    "is-obj": "^2.0.0",
    "is-regexp": "^2.1.0",
    "jest": "^25.2.0",
    "js-levenshtein": "^1.1.6",
    "klona": "^1.1.1",
    "lint-staged": "^9.5.0",
    "make-dir": "^3.0.2",
    "node-fetch": "^2.6.0",
    "parcel": "^2.0.0-alpha.3.2",
    "pg": "^7.18.2",
    "pluralize": "^8.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "set-value": "^3.0.1",
    "sql-template-tag": "^3.3.0",
    "stacktrace-parser": "0.1.7",
    "strip-ansi": "^6.0.0",
    "strip-indent": "^3.0.0",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3",
    "webpack": "5.0.0-alpha.14",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "4.0.0-alpha-4"
  },
  "peerDependencies": {
    "@prisma/cli": "*"
  },
  "peerDependenciesMeta": {
    "@prisma/cli": {
      "optional": true
    }
  }
}
